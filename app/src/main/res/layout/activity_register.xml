<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Register">


  <TextView
      android:id="@+id/textView6"
      android:layout_width="169dp"
      android:layout_height="43dp"
      android:fontFamily="@font/poppinsbold"
      android:text="Register"
      android:textAlignment="center"
      android:textColor="@color/primary"
      android:textSize="30dp"
      app:layout_constraintBottom_toBottomOf="parent"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toTopOf="parent"
      app:layout_constraintVertical_bias="0.296" />

  <EditText
      android:id="@+id/ifullname"
      android:layout_width="364dp"
      android:layout_height="52dp"
      android:background="@drawable/button_shape2"
      android:drawableLeft="@drawable/ic_baseline_person_outline_24"
      android:drawablePadding="10dp"
      android:ems="10"
      android:fontFamily="@font/poppinsregular"
      android:hint="Masukkan Nama Lengkap"
      android:inputType="textPersonName"
      android:paddingLeft="10dp"
      android:textColorHint="@color/white"
      app:layout_constraintBottom_toBottomOf="parent"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintHorizontal_bias="0.489"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toBottomOf="@+id/textView7"
      app:layout_constraintVertical_bias="0.0" />

  <TextView
      android:id="@+id/textView10"
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"
      android:fontFamily="@font/poppinssemibold"
      android:text="Email"
      android:textAlignment="center"
      android:textColor="@color/Secondary"
      app:layout_constraintBottom_toBottomOf="parent"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintHorizontal_bias="0.094"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toBottomOf="@+id/ifullname"
      app:layout_constraintVertical_bias="0.0" />

  <EditText
      android:id="@+id/iemail"
      android:layout_width="364dp"
      android:layout_height="52dp"
      android:background="@drawable/button_shape2"
      android:drawableLeft="@drawable/ic_baseline_mail_outline_24"
      android:drawablePadding="10dp"
      android:ems="10"
      android:fontFamily="@font/poppinsregular"
      android:hint="Masukkan Email"
      android:inputType="textPersonName"
      android:paddingLeft="10dp"
      android:textColorHint="@color/white"
      app:layout_constraintBottom_toBottomOf="parent"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintHorizontal_bias="0.51"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toBottomOf="@+id/textView10"
      app:layout_constraintVertical_bias="0.0" />

  <TextView
      android:id="@+id/textView8"
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"
      android:fontFamily="@font/poppinssemibold"
      android:text="Password"
      android:textAlignment="center"
      android:textColor="@color/Secondary"
      app:layout_constraintBottom_toBottomOf="parent"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintHorizontal_bias="0.102"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toBottomOf="@+id/iemail"
      app:layout_constraintVertical_bias="0.0" />

  <EditText
      android:id="@+id/ipassword"
      android:layout_width="364dp"
      android:layout_height="52dp"
      android:background="@drawable/button_shape2"
      android:drawableLeft="@drawable/ic_baseline_lock_24"
      android:drawablePadding="10dp"
      android:ems="10"
      android:fontFamily="@font/poppinsregular"
      android:hint="@string/iPass"
      android:inputType="textPassword"
      android:paddingLeft="10dp"
      android:textColorHint="@color/white"
      app:layout_constraintBottom_toBottomOf="parent"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintHorizontal_bias="0.51"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toBottomOf="@+id/textView8"
      app:layout_constraintVertical_bias="0.0" />

  <TextView
      android:id="@+id/textView9"
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"
      android:fontFamily="@font/poppinssemibold"
      android:text="Re-Password"
      android:textAlignment="center"
      android:textColor="@color/Secondary"
      app:layout_constraintBottom_toBottomOf="parent"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintHorizontal_bias="0.111"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toBottomOf="@+id/ipassword"
      app:layout_constraintVertical_bias="0.0" />

  <EditText
      android:id="@+id/ipassword2"
      android:layout_width="364dp"
      android:layout_height="52dp"
      android:background="@drawable/button_shape2"
      android:drawableLeft="@drawable/ic_baseline_lock_24"
      android:drawablePadding="10dp"
      android:ems="10"
      android:fontFamily="@font/poppinsregular"
      android:hint="Masukkan Ulang Password"
      android:inputType="textPassword"
      android:paddingLeft="10dp"
      android:textColorHint="@color/white"
      app:layout_constraintBottom_toBottomOf="parent"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintHorizontal_bias="0.51"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toBottomOf="@+id/textView9"
      app:layout_constraintVertical_bias="0.0" />

  <Button
      android:id="@+id/saveRegis"
      android:layout_width="320dp"
      android:layout_height="43dp"
      android:background="@drawable/button_shape"
      android:fontFamily="@font/poppinssemibold"
      android:text="@string/tRegis"
      android:textColor="@color/white"
      app:layout_constraintBottom_toBottomOf="parent"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintHorizontal_bias="0.494"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toTopOf="parent"
      app:layout_constraintVertical_bias="0.911" />

  <TextView
      android:id="@+id/textView7"
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"
      android:fontFamily="@font/poppinssemibold"
      android:text="Fullname"
      android:textAlignment="center"
      android:textColor="@color/Secondary"
      app:layout_constraintBottom_toBottomOf="parent"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintHorizontal_bias="0.11"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toTopOf="parent"
      app:layout_constraintVertical_bias="0.376" />

  <TextView
      android:id="@+id/haveacc"
      android:layout_width="246dp"
      android:layout_height="24dp"
      android:fontFamily="@font/poppinssemibold"
      android:text="Punya akun?"
      android:textAlignment="center"
      android:textColor="@color/Secondary"
      app:layout_constraintBottom_toBottomOf="parent"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintHorizontal_bias="0.498"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toBottomOf="@+id/saveRegis"
      app:layout_constraintVertical_bias="0.285" />

</androidx.constraintlayout.widget.ConstraintLayout>